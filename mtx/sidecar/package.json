{
  "name": "incident-management-mtx",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/cds-mtxs": "^3",
    "@sap/xsenv": "^6.1.0",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "profile": "mtx-sidecar",
    "requires": {
      "html5-host": true,
      "[mtx-sidecar]": {
        "db": {
          "kind": "sqlite",
          "impl": "@cap-js/sqlite",
          "credentials": {
            "url": "../../sqlite-local-mtx.db"
          },
          "model": [
            "../../db",
            "../../srv"
          ]
        }
      }
    }
  }
}
